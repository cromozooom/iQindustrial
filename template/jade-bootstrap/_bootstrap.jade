doctype html
html(lang="en")
	head
		meta(charset="UTF-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport",content="width=device-width, initial-scale=1")
		meta(name="description",content="")
		meta(name="author",content="")
		link(rel="icon",href="../../favicon.ico")
		title= title
		block styles
			link(rel="stylesheet",href="assets/css/app.css")
			link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.1/css/font-awesome.min.css")
			//link(rel="stylesheet",href="css/bootstrap.min.css")
	body(data-spy="scroll",data-target=".scrollspy")
		block body
		
		block footer
			#footer
				.container
					.row
						.col-xs-12.col-sm-6
							a.logo(href="http://www.minexgroup.eu/")
								img(src="assets/images/logo_orange.png" alt="Minex Romania" data-pin-nopin="true")
							p Copyright Minex Group. All Right Reserved
						.col-xs-12.col-sm-6
							ul.list-inline.text-right
								li
									a.facebook(href="#")
										i.fa.fa-facebook-official.fa-2x
								li
									a.youtube(href="#")
										i.fa.fa-youtube.fa-2x
								li
									a.twitter(href="#")
										i.fa.fa-twitter.fa-2x
								li
									a.linkedin(href="#")
										i.fa.fa-linkedin-square.fa-2x
		block scripts
			script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js")
			script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js")
